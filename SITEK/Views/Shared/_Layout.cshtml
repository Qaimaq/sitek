                            <li class="Languages">@Url.LangSwitcher(@SITEK.Resources.Resource.LangKz, ViewContext.RouteData, "Kk")</li>
                            <li class="Languages">@Url.LangSwitcher(@SITEK.Resources.Resource.LangRu, ViewContext.RouteData, "Ru")</li>
                            <li><a href="tel:+77122272072"><i class="icon-mobile-2"></i> +7 712 227 2072</a></li>
                            <li><a href="tel:+77122272062"><i class="icon-mobile-2"></i> +7 712 227 2062</a></li>
                            <li><a href="tel:+77292305161"><i class="icon-mobile-2"></i> +7 729 230 5161</a></li>
                            <li><a href="mailto:infor@sitekcaspian.kz"><i class="icon-mail-2"></i>info@sitekcaspian.com</a></li>
                        </ul>
                    </div>
                    <div class="col-md-3">
                        <ul class="social-list">
                            <li>
                                <a class="facebook sh-tooltip" data-placement="bottom" target="_blank" title="Facebook" href="https://www.facebook.com/SitekCaspian/"><i class="icon-facebook-2"></i></a>
