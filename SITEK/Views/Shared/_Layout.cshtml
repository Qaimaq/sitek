                    </div>
                </div>
            </div>
        </div>
        <div class="navbar navbar-default navbar-top">
            <div class="container">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                        <i class="icon-menu-1"></i>
                    </button>
                    <a href="@Url.Action("Index", "Home")" class="navbar-brand"><img alt="" src="~/Images/SITEK.png" style="height: 43px"></a>
                </div>
                <div class="navbar-collapse collapse">
                    <div class="search-side">
                        <a href="#" class="show-search"><i class="icon-search-1"></i></a>
                        <div class="search-form">
                            <form autocomplete="off" role="search" method="get" action="@Url.Action("SearchResult", "Home")">
                                <div class="tipue_search_right">
                                    <input type="text" name="q" id="tipue_search_input" placeholder=@SITEK.Resources.Resource.Search pattern=".{3,}" title="At least 3 characters" required>
                                </div>
                                <div style="clear: both;"></div>
                            </form>
                        </div>
                    </div>
                    <ul class="nav navbar-nav navbar-right">
                        <li>@Html.ActionLink(SITEK.Resources.Resource.menu1, "Index", "Home")</li>
