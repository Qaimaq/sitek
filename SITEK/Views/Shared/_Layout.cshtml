                    <div class="footer-widget mail-subscribe-widget">
                        <h4>@SITEK.Resources.Resource.FooterSubscribe<span class="head-line"></span></h4>
                        @using (Html.BeginForm("Subscribe", "Home", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
                        {
                            @Html.AntiForgeryToken()

                            @Html.TextBoxFor(m => m.FromEmail, new {@class = "form-control"})
                            @Html.ValidationMessageFor(m => m.FromEmail)
                            <input type="submit" style="margin-top: 10px" class="btn btn-default" value="@SITEK.Resources.Resource.Subscribe"/>
                        }
                    </div>
                    <div class="footer-widget social-widget">
                        <h4>@SITEK.Resources.Resource.FooterSocial<span class="head-line"></span></h4>
                        <ul class="social-icons">
                            <li>
                                <a class="facebook" href="https://www.facebook.com/SitekCaspian/" target="_blank"><i class="icon-facebook-2"></i></a>
                            </li>
                            <li>
                                <a class="linkdin" href="https://www.linkedin.com/in/sitek-caspian-sitek-caspian-955948102/" target="_blank"><i class="icon-linkedin-1"></i></a>
                            </li>
                        </ul>
