                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome3, "Engineering", "Home")</li>
                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome5, "Procurement", "Home")</li>
                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome7, "Construction", "Home")</li>
                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome9, "MechCompletion", "Home")</li>
                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome11, "Maintenance", "Home")</li>
                                <li>@Html.ActionLink(SITEK.Resources.Resource.indWelcome13, "Training", "Home")</li>
                            </ul>
                        </li>
                        <li>@Html.ActionLink(SITEK.Resources.Resource.menu4, "Projects", "Home")</li>
                        <li>@Html.ActionLink("HSEQ", "Hseq", "Home")</li>
                        <li>@Html.ActionLink(SITEK.Resources.Resource.menu5, "Career", "Home")</li>
                        <li>@Html.ActionLink(SITEK.Resources.Resource.menu6, "Contact", "Home")</li>
                        <span class="visible-xs Lang-switcher">@Url.LangSwitcher(@SITEK.Resources.Resource.LangEn, ViewContext.RouteData, "En")</span>
                        <span class="visible-xs Lang-switcher">@Url.LangSwitcher(@SITEK.Resources.Resource.LangKz, ViewContext.RouteData, "Kk")</span>
                        <span class="visible-xs Lang-switcher">@Url.LangSwitcher(@SITEK.Resources.Resource.LangRu, ViewContext.RouteData, "Ru")</span>
                    </ul>
                </div>
            </div>
        </div>
